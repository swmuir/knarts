            <actionGroup>
                <title value="Screening Mental Health Evaluation"/>
                <subElements>
                    <actionGroup>
                        <behaviors>
                            <behavior xsi:type="GroupSelectionBehavior" value="Any"/>
                        </behaviors>
                        <title value="Patient Health Questionnaire-Plus (PHQ-plus)"/>
                        <!--  [Technical Note: Patient Health Questionnaire-plus (PHQ-plus) must be calculated by totaling the form label values (indicated by a number following each response option) for the form labels selected by the user. Note that the following form components are adapted from Kroenke 2001.] -->
                        <!-- [Technical Note: PHQ-plus is defined as questions 1, 2, and 9 from PHQ-9.] -->
                        <subElements>
                            <actionGroup>
                                <behaviors>
                                    <behavior xsi:type="RequiredBehavior" value="Must"/>
                                </behaviors>
                                <title value="Patient Health Questionnaire-plus (PHQ-plus)"/>
                                <description value="A score of 10 is considered the threshold for mild symptoms of depression."/>
                                <subElements>
                                    <actionGroup>
                                        <behaviors>
                                            <behavior xsi:type="RequiredBehavior" value="Must"/>
                                        </behaviors>
                                        <title value="Over the past two weeks, how often have you been bothered by any of the following problems:"/>
                                        <subElements>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <documentationConcept>
                                                    <prompt value="Little interest or pleasure in doing things?"/>
                                                    <responseDataType value="Integer"/>
                                                    <responseCardinality value="Single"/>
                                                    <responseRange xsi:type="EnumerationConstraint">
                                                        <constraintType value="List"/>
                                                        <item>
                                                            <value xsi:type="elm:Literal" value="0" valueType="elm:Integer"/>
                                                            <displayText value="Not at all (score = 0)"/>
                                                        </item>
                                                        <item>
                                                            <value xsi:type="elm:Literal" value="1" valueType="elm:Integer"/>
                                                            <displayText value="Several days (score = 1)"/>
                                                        </item>
                                                        <item>
                                                            <value xsi:type="elm:Literal" value="2" valueType="elm:Integer"/>
                                                            <displayText value="More than half the days (score = 2)"/>
                                                        </item>
                                                        <item>
                                                            <value xsi:type="elm:Literal" value="3" valueType="elm:Integer"/>
                                                            <displayText value="Nearly every day (score = 3)"/>
                                                        </item>
                                                    </responseRange>
                                                </documentationConcept>
                                                <responseBinding property="PHQPlus1"/>
                                            </simpleAction>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <documentationConcept>
                                                    <prompt value="Feeling down, depressed, or hopeless?"/>
                                                    <responseDataType value="Integer"/>
                                                    <responseCardinality value="Single"/>
                                                    <responseRange xsi:type="EnumerationConstraint">
                                                        <constraintType value="List"/>
                                                        <item>
                                                            <value xsi:type="elm:Literal" value="0" valueType="elm:Integer"/>
                                                            <displayText value="Not at all (score = 0)"/>
                                                        </item>
                                                        <item>
                                                            <value xsi:type="elm:Literal" value="1" valueType="elm:Integer"/>
                                                            <displayText value="Several days (score = 1)"/>
