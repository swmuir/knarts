        <def name="insulin">
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic">
                <elm:annotation >
                    <a:s>active medication of regular insulin or isophane insulin (NPH)</a:s>
                </elm:annotation>
                <elm:codes xsi:type="elm:List">
                    <!-- regular insulin  -->
                    <elm:element xsi:type="elm:Code"
                        code="[416118004 |Administration (procedure)]
                    ->(260686004 |Method (attribute))->[129445006 |Administration - action (qualifier value)]
                    ->(363701004 |Direct substance (attribute))->[126210001 |Human insulin product (product)]"
                        display="Postcoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                    <!-- isophane insulin  -->
                    <elm:element xsi:type="elm:Code"
                        code="[416118004 |Administration (procedure)]
                    ->(260686004 |Method (attribute))->[129445006 |Administration - action (qualifier value)]
                    ->(363701004 |Direct substance (attribute))->[416089000 |Isophane insulin human (product)]"
                        display="Postcoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                </elm:codes>
            </elm:expression>
        </def>
