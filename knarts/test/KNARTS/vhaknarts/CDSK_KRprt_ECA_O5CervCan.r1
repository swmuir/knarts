        <def name="CotestsWithInPast5Years">
            <elm:annotation>
                <a:s>All co-tests done within past 5 years.</a:s>
            </elm:annotation>
            <!-- A cotest is defined as a cervical cytology exam (CervicalCytologyExams) and a cervical HPV screen (CervicalHPVScreens) done on the same day. -->
            <!-- Realistically, we probably want to relax this constraint a bit.  Perhaps done within 30 days of one another. -->
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic" dateProperty="timing.lowerBound">
                <elm:codes xsi:type="elm:List">
                    <elm:element xsi:type="elm:Code" code="416107004 |Cervical cytology test (procedure)|" display="Precoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                    <elm:element xsi:type="elm:Code" code="700152009 |Human papillomavirus screening (procedure)|" display="Precoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                </elm:codes>
                <elm:dateRange xsi:type="elm:Interval">
                    <elm:low xsi:type="elm:Add">
                        <elm:operand xsi:type="elm:Today"/>
                        <elm:operand xsi:type="elm:Quantity" unit="year" value="-5"/>
                    </elm:low>
                    <elm:high xsi:type="elm:Today"> </elm:high>
                </elm:dateRange>
            </elm:expression>
        </def>
        <def name="CotestsWithInPast10Years">
            <elm:annotation>
                <a:s>co-test results within the previous 10 years.</a:s>
            </elm:annotation>
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic" dateProperty="timing.lowerBound">
                <elm:codes xsi:type="elm:List">
                    <elm:element xsi:type="elm:Code" code="416107004 |Cervical cytology test (procedure)|" display="Precoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                    <elm:element xsi:type="elm:Code" code="416032004 |Cervicovaginal cytology normal or benign (finding)|"
                        display="Precoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                </elm:codes>
                <elm:dateRange xsi:type="elm:Interval">
                    <elm:low xsi:type="elm:Add">
                        <elm:operand xsi:type="elm:Today"/>
                        <elm:operand xsi:type="elm:Quantity" unit="year" value="-10"/>
                    </elm:low>
                    <elm:high xsi:type="elm:Today"/>
                </elm:dateRange>
            </elm:expression>
        </def>
        <def name="CervicalCytologyExam">
