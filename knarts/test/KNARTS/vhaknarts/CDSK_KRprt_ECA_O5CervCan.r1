                <a:s>cervical cytology exam within the past 3 years or patient coming due in the next 60 days</a:s>
            </elm:annotation>
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic" dateProperty="result.lowerBound">
                <elm:codes xsi:type="elm:List">
                    <elm:element xsi:type="elm:Code" code="416107004 |Cervical cytology test (procedure)|" display="Precoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                </elm:codes>
                <elm:dateRange xsi:type="elm:Interval">
                    <elm:low xsi:type="elm:Add">
                        <elm:operand xsi:type="elm:Today"/>
                        <elm:operand xsi:type="elm:Quantity" unit="year" value="-3"/>
                    </elm:low>
                    <elm:high xsi:type="elm:Add">
