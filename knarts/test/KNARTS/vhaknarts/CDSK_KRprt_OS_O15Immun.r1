            <actionGroup>
                <behaviors>
                    <behavior xsi:type="GroupSelectionBehavior" value="Any"/>
                </behaviors>
                <title value="Human Papillomavirus (HPV) Vaccine Series (0, 2, and 6 Months)"/>
