                <subElements>
                    <simpleAction xsi:type="CreateAction">
                        <textEquivalent
                            value="Zoster vaccine live injection, powder, lyophilized 0.65 mL solution subcutaneous 1 time give injection in deltoid region now"
                        />
                    </simpleAction>
                </subElements>
            </actionGroup>
