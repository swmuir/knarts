            <title value="Breast Cancer Risk Assessment"/>
            <subElements>
               <simpleAction xsi:type="CollectInformationAction">
                  <documentationConcept>
                     <prompt value="Does the patient have a personal history of any breast cancer including ductal carcinoma in situ (DCIS)?"/>
                     <itemCodes>
                        <itemCode
                           code="[a997cc03-3e99-40eb-833a-6374c7750a3a |Observation procedure (procedure)]
                           ->(363702006 |Has focus (attribute))->[254837009 |Malignant neoplasm of breast (disorder)]"
                           codeSystem="SNOMED CT">
                           <dt:displayName value="Postcoordinated Expression"/>
                        </itemCode>
                     </itemCodes>
                     <responseDataType value="Boolean"/>
                  </documentationConcept>
               </simpleAction>
               <simpleAction xsi:type="CollectInformationAction">
                  <documentationConcept>
                     <prompt value="Does the patient have a history of chest wall radiation prior to the age of 31?"/>
                     <itemCodes>
                        <itemCode code="428624002 |Radiotherapy to chest wall (procedure)|" codeSystem="SNOMED CT">
                           <dt:displayName value="Precoordianted Expression"/>
                        </itemCode>
                     </itemCodes>
                     <responseDataType value="Boolean"/>
                  </documentationConcept>
               </simpleAction>
               <simpleAction xsi:type="CollectInformationAction">
                  <documentationConcept>
                     <prompt
                        value="Does the patient have a history of known or suspected genetic mutation that increases risk of breast cancer, such as BRCA?"/>
                     <itemCodes>
                        <itemCode
                           code="[a997cc03-3e99-40eb-833a-6374c7750a3a |Observation procedure (procedure)]
                           ->(363702006 |Has focus (attribute))->[445333001 |Breast cancer genetic marker of susceptibility positive (finding)]"
                           codeSystem="SNOMED CT">
                           <dt:displayName value="Precoordianted Expression"/>
                        </itemCode>
                     </itemCodes>
                     <responseDataType value="Boolean"/>
                  </documentationConcept>
                  <responseBinding property="historyOfGeneticMutation"/>
               </simpleAction>
               <simpleAction xsi:type="CollectInformationAction">
                  <conditions>
                     <condition>
                        <logic xsi:type="elm:IsTrue">
                           <elm:operand xsi:type="elm:Property" path="historyOfGeneticMutation">
                              <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                           </elm:operand>
                        </logic>
                        <conditionRole value="ApplicableScenario"/>
                     </condition>
                  </conditions>
                  <documentationConcept>
                     <prompt value="Genetic mutation details"/>
                     <responseDataType value="String"/>
                  </documentationConcept>
               </simpleAction>
