    <actionGroup>
        <subElements>
            <actionGroup>
                <title value="Initial Screening"/>
                <subElements>
                    <actionGroup>
                        <behaviors>
                            <behavior xsi:type="GroupSelectionBehavior" value="Any"/>
                        </behaviors>
                        <title
                            value="Primary Care Posttraumatic Stress Disorder Screen for DSM-5 (PC-PTSD-5)."/>
                        <description
                            value="The PC-PTSD-5 score is calculated by totaling the numbers following each response option below. A score of >= 3 on any of the five questions about how the traumatic event(s) have affected him or her over the past month is a positive result."/>
                        <subElements>
                            <simpleAction xsi:type="CollectInformationAction">
                                <documentationConcept>
                                    <prompt
                                        value="Ask the patient the following questions: Sometimes things happen to people that are unusually or especially frightening, horrible, or traumatic. For example: a serious accident or fire; a physical or sexual assault or abuse; an earthquake or flood; a war; seeing someone be killed or seriously injured; having a loved one die through homicide or suicide. Have you ever experienced this kind of event?"/>
                                    <responseDataType value="Boolean"/>
                                </documentationConcept>
                                <responseBinding property="PTSDRisk"/>
                            </simpleAction>
                            <actionGroup>
                                <conditions>
                                    <condition>
                                        <logic xsi:type="elm:Property" path="PTSDRisk">
                                            <elm:annotation>
                                                <a:s>If true, complete the following PC-PTSD-5
                                                  questionnaire.</a:s>
                                            </elm:annotation>
                                        </logic>
                                        <conditionRole value="ApplicableScenario"/>
                                    </condition>
                                </conditions>
                                <subElements>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <behaviors>
                                            <behavior xsi:type="RequiredBehavior" value="Must"/>
                                        </behaviors>
                                        <documentationConcept>
                                            <prompt
                                                value="In the past month, have you had nightmares about the event(s) or thought about the event(s) when you did not want to?"/>
                                            <responseDataType value="Integer"/>
                                            <responseCardinality value="Single"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                  <value xsi:type="elm:Literal" valueType="Integer"
                                                  value="1"/>
                                                  <displayText value="Yes (score = 1)"/>
                                                </item>
                                                <item>
                                                  <value xsi:type="elm:Literal" valueType="Integer"
                                                  value="0"/>
                                                  <displayText value="No (score = 0)"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                        <responseBinding property="PCPTSD1"/>
                                    </simpleAction>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <behaviors>
                                            <behavior xsi:type="RequiredBehavior" value="Must"/>
                                        </behaviors>
                                        <documentationConcept>
                                            <prompt
                                                value="Tried hard not to think about the event(s) or went out of your way to avoid situations that reminded you of the event(s)?"/>
                                            <responseDataType value="Integer"/>
                                            <responseCardinality value="Single"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                  <value xsi:type="elm:Literal" valueType="Integer"
                                                  value="1"> </value>
                                                  <displayText value="Yes (score = 1)"/>
                                                </item>
                                                <item>
                                                  <value xsi:type="elm:Literal" valueType="Integer"
                                                  value="0"/>
                                                  <displayText value="No (score = 0)"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                        <responseBinding property="PCPTSD2"/>
                                    </simpleAction>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <behaviors>
                                            <behavior xsi:type="RequiredBehavior" value="Must"/>
                                        </behaviors>
                                        <documentationConcept>
                                            <prompt
                                                value="Been constantly on guard, watchful, or easily startled?"/>
                                            <responseDataType value="Integer"/>
                                            <responseCardinality value="Single"/>
                                            <responseRange xsi:type="EnumerationConstraint">
