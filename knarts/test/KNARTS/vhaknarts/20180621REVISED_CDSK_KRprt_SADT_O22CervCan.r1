        <def name="CervicalCytology">
            <elm:expression xsi:type="elm:Query">
                <elm:source alias="query">
                    <elm:expression xsi:type="elm:Instance" classType="anf:ClinicalStatement">
                        <elm:element name="statementType">
                            <elm:value xsi:type="elm:Code" code="398166005 |Performed (qualifier value)|" display="Precoordinated Expression">
                                <elm:system name="SNOMED CT"/>
                            </elm:value>
                        </elm:element>
                        <elm:element name="topic">
                            <elm:value xsi:type="elm:Code" code="416107004 |Cervical cytology test (procedure)|" display="Precoordinated Expression">
                                <elm:system name="SNOMED CT"/>
                            </elm:value>
                        </elm:element>
                    </elm:expression>
                </elm:source>
                <elm:sort>
                    <elm:by xsi:type="elm:ByColumn" path="timing.lowerBound" direction="desc"> </elm:by>
                </elm:sort>
            </elm:expression>
        </def>
        <def name="MostRecentCervicalCytology">
            <!-- Here we want the most recent cervical cytology result -->
            <elm:expression xsi:type="elm:Indexer">
                <elm:operand xsi:type="elm:ExpressionRef" name="CervicalCytology"/>
                <elm:operand xsi:type="elm:Literal" value="1" valueType="Integer"/>
            </elm:expression>
        </def>
        <def name="SecondMostRecentCervicalCytology">
            <!-- Here we want the second most recent cervical cytology result -->
            <elm:expression xsi:type="elm:Indexer">
                <elm:operand xsi:type="elm:ExpressionRef" name="CervicalCytology"/>
                <elm:operand xsi:type="elm:Literal" value="2" valueType="Integer"/>
            </elm:expression>
        </def>
        <def name="ThirdMostRecentCervicalCytology">
            <!-- Here we want the third most recent cervical cytology result -->
            <elm:expression xsi:type="elm:Indexer">
                <elm:operand xsi:type="elm:ExpressionRef" name="CervicalCytology"/>
                <elm:operand xsi:type="elm:Literal" value="3" valueType="Integer"/>
            </elm:expression>
        </def>
