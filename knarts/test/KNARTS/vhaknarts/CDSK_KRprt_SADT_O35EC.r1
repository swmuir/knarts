        <!-- Dates of prior visits.  The CCWP does not specify what visits, but assuming those to eye clinic only. -->
        <def name="Prior Visit Dates">
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic">
                <elm:codes xsi:type="elm:List">
                    <elm:element xsi:type="elm:Code"
                        code="[a997cc03-3e99-40eb-833a-6374c7750a3a |Observation procedure (procedure)] ->(363702006 |Has focus (attribute))->[185222001 |Seen in ophthalmology clinic (finding)]"
                        display="Postcoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                </elm:codes>
            </elm:expression>
        </def>
        <!-- List of current eye medications -->
        <def name="Eye Meds">
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic">
                <elm:codes xsi:type="elm:List">
                    <elm:element xsi:type="elm:Code" code="TSR-NoCode" display="Precoordinated Expression">
                        <elm:system name="SNOMED CT"/>
                    </elm:element>
                </elm:codes>
            </elm:expression>
        </def>
        <!-- List of allergies to eye medications -->
        <def name="Eye Med Allergies">
            <elm:expression xsi:type="elm:Retrieve" dataType="anf:ClinicalStatement" codeProperty="topic">
