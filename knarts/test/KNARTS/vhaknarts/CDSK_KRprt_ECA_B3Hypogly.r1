                            </elm:value>
                        </elm:element>
                        <elm:element name="topic">
                            <!-- Serum creatinine -->
                            <elm:value xsi:type="elm:Code" code="113075003 |Creatinine measurement, serum (procedure)|"
                                display="Precoordinated Expression">
                                <elm:system name="SNOMED CT"/>
                            </elm:value>
                        </elm:element>
                        <elm:element name="statementTime">
                            <elm:value xsi:type="elm:Interval">
                                <elm:low xsi:type="elm:Add">
                                    <elm:operand xsi:type="elm:Today"/>
                                    <elm:operand xsi:type="elm:Quantity" unit="day" value="150"/>
