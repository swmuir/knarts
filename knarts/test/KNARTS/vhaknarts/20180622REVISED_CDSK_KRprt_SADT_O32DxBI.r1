        <def name="LastScreeningMammogram">
            <!-- TSR: Screening mammogram -->
            <elm:annotation>
                <a:s>The last screening mammogram result</a:s>
            </elm:annotation>
            <elm:expression xsi:type="elm:Query">
                <elm:source alias="Query">
                    <elm:expression xsi:type="elm:Instance" classType="anf:ClinicalStatement">
                        <elm:element name="statementType">
                            <elm:value xsi:type="elm:Code" code="398166005 |Performed (qualifier value)|" display="Precoordinated Expression">
                                <elm:system name="SNOMED CT"/>
                            </elm:value>
                        </elm:element>
                        <elm:element name="topic">
                            <elm:value xsi:type="elm:Code" code="24623002 |Screening mammography (procedure)|" display="Precoordinated Expression">
                                <elm:system name="SNOMED CT"/>
                            </elm:value>
                        </elm:element>
                    </elm:expression>
                </elm:source>
            </elm:expression>
        </def>
