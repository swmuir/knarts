    <actionGroup>
        <subElements>
            <actionGroup>
                <title value="Screening"/>
                <description
                    value="All Veterans should be screened for homelessness (and near-future risk) annually, unless the Veteran is a long-term resident of a nursing home/long-term care facility"/>
                <subElements>
                    <actionGroup>
                        <title value="Homelessness Screening:"/>
                        <subElements>
                            <simpleAction xsi:type="CollectInformationAction">
                                <behaviors>
                                    <behavior xsi:type="RequiredBehavior" value="Must"/>
                                </behaviors>
                                <documentationConcept>
                                    <prompt value="Homeless screening performed?"/>
                                    <responseDataType value="String"/>
                                    <responseCardinality value="Single"/>
                                    <responseRange xsi:type="EnumerationConstraint">
                                        <constraintType value="List"/>
                                        <item>
                                            <displayText value="Homeless screen performed"/>
                                        </item>
                                        <item>
                                            <displayText value="Homeless screen not performed"/>
                                        </item>
                                    </responseRange>
                                </documentationConcept>
                                <responseBinding property="Homeless screen performed"/>
                            </simpleAction>
                            <actionGroup>
                                <behaviors>
                                    <behavior xsi:type="GroupSelectionBehavior" value="Any"/>
                                </behaviors>
                                <conditions>
                                    <condition>
                                        <logic xsi:type="elm:Contains">
                                            <elm:operand xsi:type="elm:Property" path="Homeless screen performed">
                                                <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                            </elm:operand>
                                            <elm:operand xsi:type="elm:Literal" valueType="String" value="Homeless screen performed"/>
                                        </logic>
                                        <conditionRole/>
                                    </condition>
                                </conditions>
                                <title value="Homeless screen"/>
                                <description
                                    value="Ask Patient: In the past 2 months, have you been living in stable housing? By that, we mean housing that is affordable, permanent, safe, decent, and livable."/>
                                <subElements>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Current housing stability"/>
                                            <itemCodes>
                                                <itemCode
                                                    code="[a997cc03-3e99-40eb-833a-6374c7750a3a |Observation procedure (procedure)]
                                                    ->(363702006 |Has focus (attribute))->[161036002 |Housing adequate (finding)]"
                                                    codeSystem="SNOMED CT">
                                                    <dt:displayName value="Postcoordinated Expression"/>
                                                </itemCode>
                                            </itemCodes>
                                            <responseDataType value="String"/>
                                            <responseCardinality value="Single"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                    <displayText value="Yes, living in stable housing"/>
                                                </item>
                                                <item>
                                                    <displayText value="No, not living in stable housing"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                        <responseBinding property="stableHousing"/>
                                    </simpleAction>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Other Details"/>
                                            <responseDataType value="String"/>
                                        </documentationConcept>
                                    </simpleAction>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <conditions>
                                            <condition>
                                                <logic xsi:type="elm:Contains">
                                                    <elm:operand xsi:type="elm:Property" path="stableHousing">
                                                        <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                                    </elm:operand>
                                                    <elm:operand xsi:type="elm:Literal" valueType="String" value="Yes"/>
                                                </logic>
                                                <conditionRole/>
                                            </condition>
                                        </conditions>
                                        <documentationConcept>
                                            <prompt value="Housing confidence"/>
                                            <responseDataType value="String"/>
                                            <responseCardinality value="Single"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                    <displayText value="Yes, confident in continuing to live in stable housing"/>
                                                </item>
                                                <item>
                                                    <displayText value="No, not confident in continuing to live in stable housing"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                        <responseBinding property="confidentLivingInStableHousing"/>
                                    </simpleAction>

                                    <actionGroup>
                                        <title value="Where have you lived for MOST of the past 2 months?"/>
                                        <subElements>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <!-- IF "No, not living in..." or "No, not confident..." Then responses below  -->
                                                <conditions>
                                                    <condition>
                                                        <logic xsi:type="elm:Contains">
                                                            <elm:operand xsi:type="elm:Property" path="confidentLivingInStableHousing">
                                                                <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                                            </elm:operand>
                                                            <elm:operand xsi:type="elm:Literal" valueType="String" value="No"/>
                                                        </logic>
                                                        <conditionRole/>
                                                    </condition>
                                                </conditions>
                                                <documentationConcept>
                                                    <prompt value="Housing situation"/>
                                                    <responseDataType value="Code"/>
                                                    <responseCardinality value="Single"/>
                                                    <responseRange xsi:type="EnumerationConstraint">
                                                        <constraintType value="List"/>
                                                        <item>
                                                            <displayText value="Apartment/house/room, no government subsidy"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="Apartment/house/room, with government subsidy"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="With friend/family"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="Motel/hotel"/>
                                                        </item>
                                                        <item>
                                                            <displayText
                                                                value="Short-term institution, such as hospital, rehab center, drug treatment center"
                                                            />
                                                        </item>
                                                        <item>
                                                            <displayText value="Homeless shelter"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="Anywhere outside, such as street, vehicle, abandoned building"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="Other"/>
                                                        </item>
                                                    </responseRange>
                                                </documentationConcept>
                                            </simpleAction>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <documentationConcept>
                                                    <prompt value="Detail"/>
                                                    <responseDataType value="String"/>
                                                </documentationConcept>
                                            </simpleAction>
                                        </subElements>
                                    </actionGroup>

                                    <actionGroup>
                                        <title value="Ask Patient: Would you like to be referred to talk more about your housing situation?"/>
                                        <subElements>
                                            <!-- Condition on "No, not living in stable housing” or “No, not confident in continuing to live in stable housing" -->
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <conditions>
                                                    <condition>
                                                        <logic xsi:type="elm:Or">
                                                            <elm:operand xsi:type="elm:Contains">
                                                                <elm:operand xsi:type="elm:Property" path="stableHousing">
                                                                    <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                                                </elm:operand>
                                                                <elm:operand xsi:type="elm:Literal" valueType="String" value="No"/>
                                                            </elm:operand>
                                                            <elm:operand xsi:type="elm:Contains">
                                                                <elm:operand xsi:type="elm:Property" path="confidentLivingInStableHousing">
                                                                    <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                                                </elm:operand>
                                                                <elm:operand xsi:type="elm:Literal" valueType="String" value="No"/>
                                                            </elm:operand>
                                                        </logic>
                                                        <conditionRole/>
                                                    </condition>
                                                </conditions>
                                                <documentationConcept>
                                                    <prompt value="Homelessness services referral preference"/>
                                                    <responseDataType value="String"/>
                                                    <responseCardinality value="Single"/>
                                                    <responseRange xsi:type="EnumerationConstraint">
                                                        <constraintType value="List"/>
                                                        <item>
                                                            <displayText value="Patient agrees to referral for homelessness services or assistance"/>
                                                        </item>
                                                        <item>
                                                            <displayText
                                                                value="Patient declines referral at this time; given information for future reference"
                                                            />
                                                        </item>
                                                    </responseRange>
                                                </documentationConcept>
                                            </simpleAction>
                                        </subElements>
                                    </actionGroup>
                                </subElements>
                            </actionGroup>
                            <actionGroup>
                                <behaviors>
                                    <behavior xsi:type="GroupSelectionBehavior" value="ExactlyOne"/>
                                </behaviors>
                                <title value="Ask Patient: What is the best way to reach you?"/>
                                <subElements>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Contact information from patient"/>
                                            <responseDataType value="String"/>
                                        </documentationConcept>
                                    </simpleAction>
                                </subElements>
                            </actionGroup>
                            <actionGroup>
                                <behaviors>
                                    <behavior xsi:type="GroupSelectionBehavior" value="OneOrMore"/>
                                </behaviors>
                                <title value="Homeless screen not peformed"/>
                                <subElements>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <!-- [Technical Note: The following options should only be displayed when “Homeless screen not performed” is selected.] -->
                                        <documentationConcept>
                                            <prompt value="Reason homeless screen not performed"/>
                                            <responseDataType value="String"/>
                                            <responseCardinality value="Multiple"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                    <displayText value="Already receiving homelessness services or assistance"/>
                                                </item>
                                                <item>
                                                    <displayText value="Long-term resident of nursing home/long-term care facility"/>
                                                    <!-- [Technical Note: End Screening IF “Long-term resident of nursing home/long-term care facility” is selected.] -->
                                                </item>
                                                <item>
                                                    <displayText value="Declines screening at this time"/>
                                                </item>
                                                <item>
                                                    <displayText value="Veteran/caregiver unable to answer"/>
                                                </item>
                                                <item>
                                                    <displayText value="Other"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                    </simpleAction>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Detail"/>
                                            <responseDataType value="String"/>
                                        </documentationConcept>
                                    </simpleAction>
                                </subElements>
                            </actionGroup>
                        </subElements>
                    </actionGroup>
                    <actionGroup>
                        <title
                            value="All Veterans should be screened for food insecurity unless the Veteran is a resident of a nursing home/long-term care facility"/>
                        <subElements>
                            <simpleAction xsi:type="CollectInformationAction">
                                <behaviors>
                                    <behavior xsi:type="RequiredBehavior" value="Must"/>
                                </behaviors>
                                <documentationConcept>
                                    <prompt value="Food insecurity screen performed?"/>
                                    <responseDataType value="String"/>
                                    <responseCardinality value="Single"/>
                                    <responseRange xsi:type="EnumerationConstraint">
                                        <constraintType value="List"/>
                                        <item>
                                            <displayText value="Food insecurity screen performed"/>
                                        </item>
                                        <item>
                                            <displayText value="Food insecurity screen not performed"/>
                                        </item>
                                    </responseRange>
                                </documentationConcept>
                            </simpleAction>
                            <actionGroup>
                                <behaviors>
                                    <behavior xsi:type="GroupSelectionBehavior" value="OneOrMore"/>
                                </behaviors>
                                <title value="Food insecurity screen"/>
                                <description
                                    value="Ask Patient “In the past 3 months, did you ever run out of food and you were not able to access more food or have the money to buy more food?"/>
                                <subElements>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Food availability"/>
                                            <responseDataType value="String"/>
                                            <responseCardinality value="Single"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                    <displayText value="Yes, food shortage and no money to buy food or access to food"/>
                                                </item>
                                                <item>
                                                    <displayText value="No food shortage"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                        <responseBinding property="foodShortage"/>
                                    </simpleAction>
                                    <actionGroup>
                                        <!-- [Technical Note: Display the following options only if “No food shortage” is chosen.] -->
                                        <title
                                            value="Ask patient “Are you confident that you will continue to have enough food or money to buy food for at least the next 3 months?"/>
                                        <subElements>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <conditions>
                                                    <condition>
                                                        <logic xsi:type="elm:Contains">
                                                            <elm:operand xsi:type="elm:Property" path="foodShortage">
                                                                <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                                            </elm:operand>
                                                            <elm:operand xsi:type="elm:Literal" valueType="String" value="No"/>
                                                        </logic>
                                                        <conditionRole/>
                                                    </condition>
                                                </conditions>
                                                <documentationConcept>
                                                    <prompt value="Food access confidence?"/>
                                                    <responseDataType value="Boolean"/>
                                                    <responseCardinality value="Single"/>
                                                    <responseRange xsi:type="EnumerationConstraint">
                                                        <constraintType value="List"/>
                                                        <item>
                                                            <displayText value="Yes, confident in continued access to food"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="No, not confident in continued access to food"/>
                                                        </item>
                                                    </responseRange>
                                                </documentationConcept>
                                            </simpleAction>
                                        </subElements>
                                    </actionGroup>
                                    <actionGroup>
                                        <title value="Ask Patient: Would you like to be referred to talk more about your food shortage situation?"/>
                                        <subElements>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <!-- [Technical Note: Display the following choices only if “Yes, food shortage and no money to buy food or access to food” or “No, not confident in continued access to food” is selected.] -->
                                                <conditions>
                                                    <condition>
                                                        <logic xsi:type="elm:Contains">
                                                            <elm:operand xsi:type="elm:Property" path="foodShortage">
                                                                <elm:source xsi:type="elm:ParameterRef" name="Responses"/>
                                                            </elm:operand>
                                                            <elm:operand xsi:type="elm:Literal" valueType="String" value="Yes"/>
                                                        </logic>
                                                        <conditionRole/>
                                                    </condition>
                                                </conditions>
                                                <documentationConcept>
                                                    <prompt value="Food shortage referral preference"/>
                                                    <responseDataType value="String"/>
                                                    <responseCardinality value="Multiple"/>
                                                    <responseRange xsi:type="EnumerationConstraint">
                                                        <constraintType value="List"/>
                                                        <item>
                                                            <displayText value="Patient agrees to referral to social services"/>
                                                        </item>
                                                        <item>
                                                            <displayText value="Patient agrees to referral to nutrition and food services"/>
                                                        </item>
                                                        <item>
                                                            <displayText
                                                                value="Patient declines referral at this time; given information for future reference"
                                                            />
                                                        </item>
                                                    </responseRange>
                                                </documentationConcept>
                                            </simpleAction>
                                            <simpleAction xsi:type="CollectInformationAction">
                                                <documentationConcept>
                                                    <prompt value="Food Insecurity Screen not performed"/>
                                                    <responseDataType value="Boolean"/>
                                                </documentationConcept>

                                            </simpleAction>
                                        </subElements>
                                    </actionGroup>
                                </subElements>
                            </actionGroup>
                            <!-- [Technical Note: The following options should only be displayed when “Food Insecurity Screen not performed” is selected.] -->

                            <actionGroup>
                                <behaviors>
                                    <behavior xsi:type="GroupSelectionBehavior" value="OneOrMore"/>
                                </behaviors>
                                <title value="Food insecurity screen not performed"/>
                                <subElements>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Reasons for not performing Food Insecurity Screen"/>
                                            <responseDataType value="String"/>
                                            <responseCardinality value="Multiple"/>
                                            <responseRange xsi:type="EnumerationConstraint">
                                                <constraintType value="List"/>
                                                <item>
                                                    <displayText
                                                        value="Already receiving services or assistance to address known food insecurity issue"/>
                                                </item>
                                                <item>
                                                    <displayText value="Resident of nursing home/long-term care facility"/>
                                                </item>
                                                <item>
                                                    <displayText value="Declines screening at this time"/>
                                                </item>
                                                <item>
                                                    <displayText value="Veteran/caregiver unable to answer"/>
                                                </item>
                                                <item>
                                                    <displayText value="Other"/>
                                                </item>
                                            </responseRange>
                                        </documentationConcept>
                                    </simpleAction>
                                    <simpleAction xsi:type="CollectInformationAction">
                                        <documentationConcept>
                                            <prompt value="Detail"/>
                                            <responseDataType value="String"/>
                                        </documentationConcept>
                                    </simpleAction>
                                </subElements>
                            </actionGroup>
                        </subElements>
                    </actionGroup>
                </subElements>
            </actionGroup>
            <actionGroup>
                <title value="Plan"/>
                <subElements>
                    <actionGroup>
                        <behaviors>
                            <behavior xsi:type="GroupSelectionBehavior" value="Any"/>
                        </behaviors>
                        <title value="Provide homelessness educational materials"/>
