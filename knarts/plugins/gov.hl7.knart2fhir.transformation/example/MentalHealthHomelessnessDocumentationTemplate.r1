                            </simpleAction>
                            <simpleAction xsi:type="CollectInformationAction">
                                <documentationConcept>
                                    <prompt value="Describe in detail the Veteran’s current living situation"/>
                                    <responseDataType value="String"/>
                                </documentationConcept>
                            </simpleAction>
                        </subElements>
                    </actionGroup>
                    <actionGroup>
                        <behaviors>
                            <behavior xsi:type="GroupSelectionBehavior" value="Any"/>
                        </behaviors>
                        <title value="Order consult for social work or for nutrition and food services"/>
                        <subElements>
                            <simpleAction xsi:type="CollectInformationAction">
                                <documentationConcept>
                                    <prompt value="Reason for request"/>
                                    <itemCodes>
                                        <itemCode code="1061000205108 |Social services consultation and report (procedure)" codeSystem="SNOMED CT">
                                            <dt:displayName value="Precoordinated Expression"/>
                                        </itemCode>
                                        <itemCode
                                            code="c86bf351-ec7e-4310-ae35-5cfc41d3f9fe |Nutrition and food services consultation and report (procedure)"
                                            codeSystem="SNOMED CT">
                                            <dt:displayName value="Precoordinated Expression"/>
                                        </itemCode>
                                    </itemCodes>
                                    <responseDataType value="Code"/>
                                    <responseCardinality value="Single"/>
                                    <responseRange xsi:type="EnumerationConstraint">
                                        <constraintType value="List"/>
                                        <item>
                                            <displayText
                                                value="Veteran has been identified as having a current food shortage or a food shortage within the past 3 months"
                                            />
                                        </item>
                                        <item>
                                            <displayText
                                                value="Veteran has been identified as being at risk of a food shortage within the next 90 days"/>
                                        </item>
