                <elm:expression xsi:type="elm:Retrieve" dataType="ClinicalStatement" codeProperty="topic">
                    <elm:annotation >
                        <a:s>Submission of the "Mental Health – Iraq &amp; Afghanistan Post-Deployment Screen" documentation template.</a:s>
                    </elm:annotation>
                    <elm:codes xsi:type="elm:List">
