        <subElements>
            <!-- Section 3.2.1: Unexplained Fevers -->
            <actionGroup>
                <conditions>
                    <condition>
                        <logic xsi:type="elm:Exists">
                            <elm:annotation >
                                <a:s>Affirmative response to: Unexplained fevers since serving in the area of conflict?</a:s>
                            </elm:annotation>
                            <elm:operand xsi:type="elm:ExpressionRef" name="UnexplainedFevers"/>
                        </logic>
                        <conditionRole value="ApplicableScenario"/>
