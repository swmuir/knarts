                                    <elm:system name="SNOMED CT"/>
                                </elm:value>
                            </elm:element>
                            <elm:element name="topic">
                                <elm:value xsi:type="elm:Code" code="TSR-NoCode" display="Precoordinated Expression">
                                    <elm:system name="SNOMED CT"/>
                                </elm:value>
                            </elm:element>
                        </actionSentence>
                    </simpleAction>
                </subElements>
            </actionGroup>
        </subElements>
    </actionGroup>
